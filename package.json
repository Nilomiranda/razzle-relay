{
  "name": "my-razzle-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "update-schema": "yarn upgrade @octokit/graphql-schema",
    "relay": "relay-compiler --src ./src --schema ./node_modules/@octokit/graphql-schema/schema.graphql"
  },
  "dependencies": {
    "@octokit/graphql-schema": "^6.64.0",
    "express": "^4.17.1",
    "graphql-tag": "^2.10.2",
    "razzle": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-relay": "^8.0.0",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "babel-plugin-relay": "^8.0.0",
    "graphql": "^14.6.0",
    "relay-compiler": "^8.0.0",
    "relay-config": "^8.0.0"
  }
}
